def find_max_element(lst):
    if not lst:
        return None  # Return None for an empty list Not 
    else:
        max_element = lst[0]
        for element in lst:
            if element > max_element:
                max_element = element
        return max_element
#left rigver ones
# Example usage
numbers_list = [8, 3, 11, 5, 2, 10]

max_number = find_max_element(numbers_list)

if max_number is not None:
    print(f"The maximum element in the list is: {max_number}")
else:
    print("The list is empty.")
